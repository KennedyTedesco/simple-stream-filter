{
  "name": "kennedytedesco/simple-stream-filter",
  "description": "Painless stream filtering in PHP.",
  "keywords": [
    "stream",
    "streams",
    "php",
    "filter"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Kennedy Tedesco",
      "email": "kennedyt.tw@gmail.com"
    }
  ],
  "require": {
    "php": "^7.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "friendsofphp/php-cs-fixer": "^2.15"
  },
  "autoload": {
    "psr-4": {
      "SimpleStreamFilter\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "cs": [
      "vendor/bin/php-cs-fixer fix --allow-risky yes"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
